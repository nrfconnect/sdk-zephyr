/ {
	test {
		#address-cells = <1>;
		#size-cells = <1>;

		test_gpio: gpio@deadbeef {
			compatible = "vnd,gpio";
			gpio-controller;
			reg = <0xdeadbeef 0x1000>;
			#gpio-cells = <0x2>;
			status = "okay";
		};
	};
};

/ {
	test_uln2003_motor_cluster: uln2003_motor_cluster {
		compatible = "gpio-steppers";
		status = "okay";

		motor_1: motor_1 {
			micro-step-res = <1>;
			gpios = <&test_gpio 0 0>,
				<&test_gpio 0 0>,
				<&test_gpio 0 0>,
				<&test_gpio 0 0>;
		};
	};
};
