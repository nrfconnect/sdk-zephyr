/dts-v1/;

#include <atmel/sam3x.dtsi>

/ {
	model = "Arduino Due with an Atmel SAM3X8E SoC";
	compatible = "arduino,due", "atmel,sam3x8e", "atmel,sam3x";

	aliases {
		wdog = &wdog;
		uart-0 = &uart0;
		i2c-0 = &i2c0;
		i2c-1 = &i2c1;
	};

	chosen {
		zephyr,sram = &sram0;
		zephyr,flash = &flash0;
		zephyr,console = &uart0;
		zephyr,shell-uart = &uart0;
	};
};

&wdog {
	status = "ok";
};


&i2c0 {
	status = "ok";
};

&i2c1 {
	status = "ok";
};

&uart0 {
	status = "ok";
	current-speed = <115200>;
};
