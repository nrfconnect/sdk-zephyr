/dts-v1/;

#include <nxp/nxp_kw40z.dtsi>

/ {
	model = "Hexiwear KW40 board";
	compatible = "nxp,kw40z", "nxp,mkw40z4";

	chosen {
		zephyr,sram = &sram0;
		zephyr,flash = &flash0;
	};
};

&adc0 {
	status = "okay";
};

&i2c1 {
	status = "okay";
};

&lpuart0 {
	status = "okay";
	current-speed = <115200>;
};
