if BOARD_STM32F769I_DISCO

config BOARD
	default "stm32f769i_disco"

if UART_CONSOLE

config UART_STM32_PORT_1
	default y

endif # UART_CONSOLE

if SERIAL

config UART_STM32_PORT_6
	default y

endif # SERIAL

if I2C

config I2C_1
	default y

endif # I2C

if SPI

config SPI_STM32_INTERRUPT
	default y

config SPI_2
	default y

endif # SPI

endif # BOARD_STM32F769I_DISCO
