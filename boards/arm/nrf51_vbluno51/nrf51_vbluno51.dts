/dts-v1/;
#include <nordic/nrf51822.dtsi>

/ {
	model = "VNG VBLUno51 BLE board";
	compatible = "vng,vbluno51", "nordic,nrf51822-qfac",
		     "nordic,nrf51822";

	chosen {
		zephyr,console = &uart0;
		zephyr,sram = &sram0;
		zephyr,flash = &flash0;
	};
};

&uart0 {
	current-speed = <115200>;
	status = "ok";
};

&i2c0 {
	status = "ok";
	clock-frequency = <I2C_BITRATE_FAST>;
};
