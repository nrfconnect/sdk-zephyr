/* Add SPI1 support on Particle Mesh via nRF52840 SPI3
 *
 * NOTE: This file is replicated in particle_{argon,boron,xenon}.
 * Changes should be made in all instances. */

&spi3 { /* feather SPI */
	status = "ok";
	sck-pin = <33>;
	mosi-pin = <34>;
	miso-pin = <40>;
};

