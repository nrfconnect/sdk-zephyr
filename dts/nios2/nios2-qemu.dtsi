#include "skeleton.dtsi"

/ {
	cpus {
		#address-cells = <1>;
		#size-cells = <0>;

		cpu@0 {
			device_type = "cpu";
			compatible = "qemu,nios2";
			reg = <0>;
		};

	};

	flash0: flash@0 {
		compatible = "soc-nv-flash";
		reg = <0x420000 0x20000>;
	};

	sram0: memory@400000 {
		device_type = "memory";
		compatible = "mmio-sram";
		reg = <0x400000 0x20000>;
	};

	soc {
		#address-cells = <1>;
		#size-cells = <1>;
		compatible = "simple-bus";
		ranges;

		jtag_uart: uart@201000 {
			compatible = "altera,jtag-uart";
			reg = <0x201000 0x400>;
			label = "jtag_uart0";

			status = "disabled";
		};

		ns16550_uart: uart@440000 {
			compatible = "ns16550";
			reg = <0x440000 0x400>;
			label = "UART_0";

			status = "disabled";
		};

	};
};
