&i2c0 {

	hdc1008: hdc1008@40 {
		compatible = "ti,hdc1008";
		reg = <0x40>;
		label = "HDC1008";
		drdy-gpios = <&gpio0 3 0>;
	};

	bmp280@76 {
		compatible = "bosch,bme280";
		reg = <0x76>;
		label = "BMP280";
	};

};
