/ {
	zephyr,user {
		left-gpios = <&edge_connector 13 GPIO_ACTIVE_HIGH>;
		right-gpios = <&edge_connector 14 GPIO_ACTIVE_HIGH>;
	};
};

&i2c0 {
	motorctl: motor-controller@10 {
		compatible = "motor-controller";
		reg = <0x10>;
	};
};
