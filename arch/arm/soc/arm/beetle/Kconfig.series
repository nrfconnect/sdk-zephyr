config SOC_SERIES_BEETLE
	bool "ARM Beetle MCU Series"
	depends on ARM
	select CPU_CORTEX_M3
	select SOC_FAMILY_ARM
	select CPU_HAS_SYSTICK
	select CPU_HAS_MPU
	help
	 Enable support for Beetle MCU Series
